{
  "name": "dreampark-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf static/dist && better-npm-run build",
    "dev": "rimraf static/dist && concurrently --kill-others \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --colors --progress --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "keywords": [
    "react"
  ],
  "author": "Wang Chao",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.11.1",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "js-sha256": "^0.6.0",
    "prop-types": "^15.5.10",
    "qrcode.react": "^0.7.1",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.2",
    "redux": "^3.7.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "superagent": "^3.5.2",
    "universal-cookie": "^2.0.8",
    "universal-cookie-express": "^2.0.6"
  },
