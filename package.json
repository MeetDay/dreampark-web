{
  "name": "dreampark-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && better-npm-run build",
    "dev": "concurrently --kill-others \"npm run start-dev\"",
    "start-dev": "better-npm-run start-dev",
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --colors --progress --config webpack/webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "keywords": [
    "react"
  ],
  "author": "Wang Chao",
  "license": "ISC",
  "dependencies": {
    "antd": "^2.11.1",
    "compression": "^1.6.2",
    "express": "^4.15.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.0",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "scroll-behavior": "^0.9.3",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.0.15",
    "concurrently": "^3.4.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "less-loader": "^4.0.4",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0"
  }
}
